#6 The Late Late Show with James Corden     163
#7 Team Coco                                106
#Finding channels to be most featured on trending page
topChannels = USvideos %>% group_by(channel_title) %>% count(channel_title) %>% arrange(desc(n))
head(topChannels, 20)
